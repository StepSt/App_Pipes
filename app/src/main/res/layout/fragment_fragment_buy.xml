<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Fragments.FragmentCalc"
    android:background="@drawable/grid"
    xmlns:foo="http://schemas.android.com/apk/res-auto">

    <!-- TODO: Update blank fragment layout -->
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top|left|bottom|right"
        android:background="@drawable/rounded"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp">
        <admin.example.com.pipes_v2.TextViewPlus
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ваш заказ:"
            android:id="@+id/textView19"
            android:layout_gravity="left|top"
            foo:customFont="GOST type A.ttf"
            android:textSize="30dp"
            android:textColor="@color/white"/>
        <admin.example.com.pipes_v2.TextViewPlus
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:text="Количество элементов в заказе"
            android:id="@+id/textView20"
            android:layout_gravity="left|top"
            foo:customFont="GOST type A.ttf"
            android:textSize="20dp"
            android:textColor="@color/white"/>
        <admin.example.com.pipes_v2.TextViewPlus
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_marginRight="30dp"
            android:text="0"
            android:id="@+id/textView18"
            android:layout_gravity="right|top"
            foo:customFont="GOST type A.ttf"
            android:textSize="20dp"
            android:textColor="@color/white"/>
        <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginTop="60dp"
            android:id="@+id/one"
            android:layout_marginBottom="50dp"/>
            <ListView
                android:id="@+id/lvMain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="150dp">
            </ListView>

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Доставка"
                android:id="@+id/checkBox_delivery"
                android:layout_marginBottom="150dp"
                android:layout_gravity="bottom" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="bottom"
            android:id="@+id/Delivery">

            <admin.example.com.pipes_v2.TextViewPlus
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Введите адрес доставки"
                android:id="@+id/txt_addressDelivery"
                foo:customFont="GOST type A.ttf"
                android:textSize="20dp"
                android:textColor="@color/white"
                android:gravity="center_vertical|center"
                android:clickable="true"
                android:visibility="visible" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <admin.example.com.pipes_v2.TextViewPlus
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4"
                    android:text="Город"
                    android:id="@+id/textView2"
                    android:inputType="text"
                    android:textAlignment="center"
                    foo:customFont="GOST type A.ttf"
                    android:textSize="25dp"
                    android:textColor="@color/white"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4"
                    android:id="@+id/editText_CitiDelivery"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <admin.example.com.pipes_v2.TextViewPlus
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4"
                    android:text="Адрес"
                    android:id="@+id/textView3"
                    android:inputType="text"
                    android:textAlignment="center"
                    foo:customFont="GOST type A.ttf"
                    android:textSize="25dp"
                    android:textColor="@color/white"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4"
                    android:id="@+id/editText_AdressDelivery"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <admin.example.com.pipes_v2.TextViewPlus
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4"
                    android:text="Примечение"
                    android:id="@+id/textView4"
                    android:inputType="text"
                    android:textAlignment="center"
                    foo:customFont="GOST type A.ttf"
                    android:textSize="25dp"
                    android:textColor="@color/white"/>
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4"
                    android:id="@+id/editText_PrimDelivery"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:id="@+id/two">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:id="@+id/button_order"
                android:layout_marginTop="5dp"
                android:text="@string/box"
                android:visibility="gone">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:id="@+id/tree"
            android:layout_gravity="bottom"
            android:layout_weight="1">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:id="@+id/clear"
                android:layout_gravity="top"
                android:background="@drawable/rounded"
                android:layout_weight="1"
                android:clickable="true">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:id="@+id/icon_clear"
                    android:src="@drawable/ic_clear_white_48px"/>
                <admin.example.com.pipes_v2.TextViewPlus
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Очистить"
                    android:id="@+id/textView24"
                    foo:customFont="GOST type A.ttf"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center"
                    android:layout_gravity="bottom|right"
                    android:clickable="true"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:id="@+id/sent"
                android:layout_gravity="top"
                android:background="@drawable/rounded"
                android:layout_weight="1"
                android:clickable="true">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:id="@+id/icon_by"
                    android:src="@drawable/ic_send_white_48px"/>
                <admin.example.com.pipes_v2.TextViewPlus
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Отправить"
                    android:id="@+id/textView17"
                    foo:customFont="GOST type A.ttf"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:gravity="center_vertical|center"
                    android:layout_gravity="bottom|right"
                    android:clickable="true"/>
            </LinearLayout>

        </LinearLayout>

    </FrameLayout>
</FrameLayout>
