    <com.google.android.gms.maps.MapView
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:apiKey="AIzaSyA4C-btj0xhUc0hQ2dSnjZfxaxDyx86Y-8"
